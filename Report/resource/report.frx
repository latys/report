using FastReport.Barcode;
using FastReport.Table;
using FastReport.Utils;

namespace FastReport
{
  public class ReportScript
  {

    private void Table1_ManualBuild(object sender, EventArgs e)
    {
      // get the data source by its name
      DataSourceBase rowData = Report.GetDataSource(&quot;rateView&quot;);
      // initialize it
      rowData.Init();
      
      DataSourceBase rowData1 = Report.GetDataSource(&quot;unCommit&quot;);
      // initialize it
      rowData1.Init();
      
    
      // printing the table header
      Table1.PrintRow(0);
      Table1.PrintColumns();
      // loop through the data source rows
      while (rowData.HasMoreRows)
      {
        // printing the table row
        Table1.PrintRow(1);
        Table1.PrintColumns();
        // select the next data row
        rowData.Next();
      } 
      // printing the table footer
      Table1.PrintRow(2);
      Table1.PrintColumns();
      
      Table1.PrintRow(3);
      Table1.PrintColumns();
      while (rowData1.HasMoreRows)
      {
        // printing the table row
        Table1.PrintRow(4);
        Table1.PrintColumns();
        // select the next data row
        rowData1.Next();
      } 
      // printing the table footer
      Table1.PrintRow(5);
      Table1.PrintColumns();
      
    
      
    }

    
  }
}
</ScriptText>
